<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Analyze Stack Trace...">
        <Item>
          <IdeaActionID>Unscramble</IdeaActionID>
          <ShortCut>'Alt+E, T' or 'Alt+E, Alt+T'</ShortCut>
          <Description>Analyze Stack Trace...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'Attach to Unity Editor'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Alt+F5'</ShortCut>
          <Description>Debug 'Attach to Unity Editor'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'F12'</ShortCut>
          <Description>Declaration</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Explorer">
        <Item>
          <IdeaActionID>ActivateExplorerToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Explorer</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'Shift+F12'</ShortCut>
          <Description>Find Usages</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'F5'</ShortCut>
          <Description>Resume Program</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Intention Actions">
        <Item>
          <IdeaActionID>ShowIntentionActions</IdeaActionID>
          <ShortCut>'Alt+Enter'</ShortCut>
          <Description>Show Intention Actions</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Step Over">
        <Item>
          <IdeaActionID>StepOver</IdeaActionID>
          <ShortCut>'F10'</ShortCut>
          <Description>Step Over</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Attach to Unity Editor'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Shift+F5'</ShortCut>
          <Description>Stop 'Attach to Unity Editor'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'F9'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Back">
        <Item>
          <IdeaActionID>Back</IdeaActionID>
          <ShortCut>'Ctrl+Minus' or 'Ctrl+NumPad -'</ShortCut>
          <Description>Back</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>